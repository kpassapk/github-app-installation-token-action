{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "Action that Create GitHub App Installation Token",
  "main": "lib/main.js",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "build": "npm run embed && esbuild --bundle ./src/index.ts --platform=node --target=node12 --main-fields=main --outdir=dist/",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm test",
    "embed": "npx ts-node src/bin/embedDeps.ts",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "YourNameOrOrganization",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@types/nock": "^11.1.0",
    "debug": "^4.3.4",
    "github-app-installation-token": "^1.2.0",
    "husky": "^8.0.3",
    "node-rsa": "^1.1.1"
  },
  "devDependencies": {
    "@octokit/types": "^5.0.1",
    "@types/debug": "^4.1.7",
    "@types/jest": "^24.0.23",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^12.20.55",
    "@types/node-rsa": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "auto-changelog": "2.4.0",
    "esbuild": "0.16.7",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-github": "^4.6.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "^24.9.0",
    "js-yaml": "^3.14.1",
    "nock": "^13.0.4",
    "prettier": "^2.8.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^10.9.1",
    "typescript": "4.2.4"
  }
}
